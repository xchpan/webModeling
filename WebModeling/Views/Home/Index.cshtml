<div id="all-plants-form" title="All Plants" ng-app="allPlants">
    <form ng-controller="AllPlantController">
        <table class="all-plants">
            <thead>
                <tr><th>Name</th><th>Creator</th><th>Created Time</th><th>Description</th><th></th></tr>
            </thead>
            <tr ng-repeat="s in plants">
                <td><input type="text" ng-model="s.Name" ng-change="updatePlant(s.Id)" /></td>
                <td>{{s.Creator}}</td>
                <td>{{s.CreateDatetime}}</td>
                <td><input type="text" ng-model="s.Description" ng-change="updatePlant(s.Id)" /></td>
                <td><button ng-click="deletePlant(s.Id)">&#x2718</button></td>
            </tr>
        </table>
        <fieldset>
            <legend></legend>
            <input type="button" value="createPlant" ng-click="createPlant()" />
        </fieldset>
    </form>
</div>
